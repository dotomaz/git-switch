{
  "name": "@dotomaz/git-switch",
  "author": "Tomaž Dobrišek (dotomaz@gmail.com)",
  "version": "0.0.4",
  "description": "Command that switches between latest branches that you are working on.",
  "license": "MIT",
  "main": "bin/git-switch.js",
  "bin": {
    "git-switch": "./bin/git-switch.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dotomaz/git-switch.git"
  },
  "scripts": {
    "start": "node bin/git-switch.js",
    "lint": "eslint bin/*",
    "lint-fix": "eslint bin/* --fix",
    "lint-fix-dry-run": "eslint bin/* --fix-dry-run ",
    "semantic-release": "semantic-release"
  },
  "keywords": [
    "git",
    "branch",
    "latest",
    "switch"
  ],
  "dependencies": {
    "chalk": "^4.1.0",
    "inquirer": "^7.3.3"
  },
  "devDependencies": {
    "eslint": "^7.7.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.0",
    "semantic-release": "^17.1.1"
  }
}
